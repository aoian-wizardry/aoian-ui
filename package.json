    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.5",
    "@vercel/analytics": "^1.5.0",
    "ai": "^4.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
